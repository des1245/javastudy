package ex02_String;

import java.util.Scanner;

//2. 정수/ 실수 판별하기
//입력 >>> 120
// 120은 정수입니다.
//입력>>>3.14
//3.14는 실수 입니다.
public class Quiz02 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		String n ="";
		System.out.println("입력");
		n = sc.next();
		System.out.println(n.str);
	}
}
