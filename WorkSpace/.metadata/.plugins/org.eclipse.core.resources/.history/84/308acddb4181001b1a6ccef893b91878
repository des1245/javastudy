package ex01_random;

import java.util.Scanner;

//2.UpDown게임
//1~10000 사이의 난수가 발생되면 사용자가 해당 난수를 맞히는 게임
//입력 >>> 5000
//Up
//입력 >>>7500
//Down
//입력 >>> 7499
//정답. 총 3번만에 정답

public class Quiz02 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int rand =(int)(Math.random()*10000)+1;//난수 발생
		
	}
}
