package quiz02;

public class Circle {
	
	//field
	private Coord center;
	private double radius;
	
	//constructor
	public Circle (Coord center,double radius) {
		this.center =center;
		this.radius =radius;
	}
	
	//method
	public double getArea() {
		return Math.PI * Math.pow(radius,2);
	}
	public void info() {
		System.out.println("중심 좌표");
		center.info();
	}
}
